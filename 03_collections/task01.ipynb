   "source": [
    "grades = [70, 75, 80, 85, 90] #[85, 90, 78, 92, 88]\n",
    "\n",
    "avg = sum(grades) / len(grades)\n",
    "\n",
    "print(f\"平均点:{avg:.1f}\")\n",
    "print(f\"最高点:{max(grades)}\")\n",
    "print(f\"最低点:{min(grades)}\")\n",
    "\n",
    "count = 0\n",
    "for i in grades:\n",
    "    if i >= avg:\n",
    "        count +=1\n",
    "\n",
    "print(f\"平均以上の学生数:{count}\")"
   ]
  }
